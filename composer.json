{
    "name": "zakriyarahman/laravel-mysql-optimize",
    "type": "library",
    "description": "A laravel package that optimizes mysql database tables.",
    "keywords": ["database","laravel","optimize","mysql","table"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zak Rahman",
            "email": "zakriyarahman@gmail.com",
            "homepage": "https://github.com/zakriyarahman",
            "role": "Author"
        }
    ],
    "require": {
        "illuminate/console": "^7.0",
        "illuminate/database": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Zaks\\MySQLOptimier\\":"src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zaks\\MySQLOptimier\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaks\\MySQLOptimier\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
